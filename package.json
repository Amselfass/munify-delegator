{
  "name": "munify-delegator",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run dev:server\" \"bun run dev:docker\"",
    "dev:server": "vite",
    "dev:docker": "docker compose -f ./dev.docker-compose.yml up",
    "vite": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "tsc --noEmit",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@fontsource/outfit": "^5.0.14",
    "@fontsource/roboto-mono": "^5.0.19",
    "@fontsource/vollkorn": "^5.0.21",
    "@sveltejs/kit": "^2.5.24",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.10",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "svelte": "^5.0.0-next.238",
    "svelte-check": "^3.8.6",
    "tailwindcss": "^3.4.10",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "@casl/ability": "^6.7.1",
    "@casl/prisma": "^1.4.1",
    "@elysiajs/eden": "^1.1.2",
    "@inlang/paraglide-sveltekit": "0.11.0",
    "@sinclair/typebox": "^0.33.7",
    "concurrently": "^8.2.2",
    "convert-iso-codes": "^1.0.9",
    "cryptr": "^6.3.0",
    "flag-icons": "^7.2.3",
    "openid-client": "^5.6.5",
    "svelte-adapter-bun": "^0.5.2",
    "world-countries": "^5.0.0",
    "@inlang/paraglide-js": "1.11.2",
    "@prisma/client": "^5.18.0",
    "elysia": "^1.1.7",
    "prismabox": "^1.1.9",
    "prisma": "^5.18.0",
    "jose": "^5.7.0",
    "mermaid": "^11.0.2"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "esbuild": "^0.23.1",
    "jose": "^5.7.0",
    "mermaid": "^11.0.2",
    "numeral": "^2.0.6",
    "svelte-headless-table": "^0.18.2"
  }
