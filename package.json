{
  "name": "munify-delegator",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run dev:server\" \"bun run dev:docker\"",
    "dev:server": "vite",
    "dev:docker": "docker compose -f ./dev.docker-compose.yml up",
    "vite": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@fontsource/outfit": "^5.0.13",
    "@fontsource/roboto-mono": "^5.0.18",
    "@fontsource/vollkorn": "^5.0.20",
    "@sveltejs/kit": "^2.5.18",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.12.10",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "svelte": "^5.0.0-next.175",
    "svelte-check": "^3.8.4",
    "tailwindcss": "^3.4.5",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "@casl/ability": "^6.7.1",
    "@casl/prisma": "^1.4.1",
    "@elysiajs/eden": "^1.0.14",
    "@inlang/paraglide-sveltekit": "0.11.0",
    "@sinclair/typebox": "^0.32.34",
    "concurrently": "^8.2.2",
    "convert-iso-codes": "^1.0.9",
    "cryptr": "^6.3.0",
    "flag-icons": "^7.2.3",
    "openid-client": "^5.6.5",
    "svelte-adapter-bun": "^0.5.2",
    "world-countries": "^5.0.0",
    "@inlang/paraglide-js": "1.11.2",
    "@prisma/client": "^5.16.2",
    "elysia": "^1.0.27",
    "prismabox": "^1.1.3",
    "prisma": "^5.16.2"
  }
}